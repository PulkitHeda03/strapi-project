{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "metaTitle": {
      "type": "string",
      "required": true
    },
    "metaDescription": {
      "type": "text",
      "required": true
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "publishAt": {
      "type": "datetime",
      "required": true,
      "default": "2025-12-30T05:30:28.458Z"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "metaKeyword": {
      "type": "string"
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": true
    },
    "FAQ": {
      "type": "component",
      "component": "faq.faq-section",
      "repeatable": true
    },
    "canonicalURL": {
      "type": "string",
      "required": true
    }
  }
}
